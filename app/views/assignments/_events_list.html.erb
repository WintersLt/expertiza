
<table>
  <div id="url" style="">
    <tr>
      <th> Artifact Name </th>
      <th> Event</th>
      <th> Event Time </th>
          </tr>
    
    <% @events.each do |event|  %>
            <tr>
              <% @participant = AssignmentParticipant.find(event.participant_id) %>
              <% team_id_event = TeamsUser.team_id( @participant.parent_id,
                                                    @participant.user_id) %>
              <% if @teams.id == team_id_event %>
                  <td><%= event.artifact_name %> &nbsp;  &nbsp;</td>
                  <td><%= event.event%> &nbsp;  &nbsp;</td>
                  <td><%= event.event_time%> &nbsp;  &nbsp;</td>
              <% end %>
            </tr>
    <% end %>
        
  </div>
</table>
<%= link_to "Hide submission history", list_submissions_path(:id => @assignment.id) =%>