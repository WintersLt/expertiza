
<table>
  <div id="url" style="">
    <tr>
      <th> Artifact Name </th>
      <th> Event</th>
      <th> Event Time </th>
    </tr>
    <% @events.each do |event|  %>
            <tr>
              <% if participant == event.participant_id %>
                  <td><%= event.artifact_name %> &nbsp;  &nbsp;</td>
                  <td><%= event.event%> &nbsp;  &nbsp;</td>
                  <td><%= event.event_time%> &nbsp;  &nbsp;</td>
              <% end %>
            </tr>
    <% end %>
  </div>
</table>